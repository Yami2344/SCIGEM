{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach to Chrome",
      "type": "chrome",
      "request": "attach",
      "port": 9222,
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "Launch Chrome (localhost)",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:8080",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "Open local index.html",
      "type": "chrome",
      "request": "launch",
      "file": "C:/Users/yamit/OneDrive/Desktop/SCIGEM/index.html"
    }
  ]
}
